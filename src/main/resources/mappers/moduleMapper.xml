<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pers.tom.docwarehouse.mapper.ModuleMapper">

    <select id="existByName" parameterType="string" resultType="boolean">
        select count(1)
        from `module`
        where `name` = #{name}
        limit 1
    </select>
</mapper>